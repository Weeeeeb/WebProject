<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>meets</display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SaveNewEvent</servlet-name>
    <servlet-class>controller.SaveNewEvent</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>JoinAEvent</servlet-name>
    <servlet-class>controller.JoinAEvent</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ProcessLogin</servlet-name>
    <servlet-class>controller.ProcessLogin</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SavaTimes</servlet-name>
    <servlet-class>controller.SavaTimes</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SaveNewEvent</servlet-name>
    <url-pattern>/servlet/SaveNewEvent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JoinAEvent</servlet-name>
    <url-pattern>/servlet/JoinAEvent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessLogin</servlet-name>
    <url-pattern>/servlet/ProcessLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SavaTimes</servlet-name>
    <url-pattern>/servlet/SavaTimes</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <error-page>
        <error-code>404</error-code>
        <location>/404error.html</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/500error.html</location>
    </error-page>
    
  <error-page>
        <exception-type>java.lang.Exception</exception-type>
        <location>/error.html</location>
    </error-page>
    
    
</web-app>